$(function() {
  $('#amazonbtn').mousedown(function() {
      $("#amazonbtn").attr('src',"<%= image_path('amazonbuttonclick.jpg') %>");
      console.log("<%= image_path('amazonbuttonclick.jpg') %>");
  }).bind('mouseup mouseleave', function() {
      $("#amazonbtn").attr('src',"<%= image_path('amazonbutton.jpg') %>");
      console.log("<%= image_path('amazonbutton.jpg') %>");
  });
});
